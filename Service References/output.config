<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IWorkingHoursService" />
                <binding name="BasicHttpBinding_IProjectSyncService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/MPSTestHost" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IWorkingHoursService"
                contract="IWorkingHoursService" name="BasicHttpBinding_IWorkingHoursService" />
            <endpoint address="http://localhost:8080/MPSTestHost" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IProjectSyncService"
                contract="IProjectSyncService" name="BasicHttpBinding_IProjectSyncService" />
        </client>
    </system.serviceModel>
</configuration>